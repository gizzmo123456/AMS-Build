{
  "build-name-format": "{project}_{build_hash}_build_{build_index}",
  "docker": {
    "image": "gableroux/unity3d:2018.4.2f1-windows",
    "args": "-it --rm",
    "project-dest": "/root/project",
    "build-output-dest": "/root/project/unityBuild/Builds/StandaloneWindows",
    "stop-timeout": 10
  },
  "webhook": {
    "name": "master",
    "authorized-actors": [
      "ashley sands"
    ]
  },
  "prepare-build": {
    "master-dir-commands": [
      "cd testCIGame/",
      "sudo git pull origin master "
    ],
    "get-git-hash": true,
    "build-dir-commands": [
      "cd testCIGame/",
      "sudo git checkout --detach {git_hash}"
    ]
  },
  "environment": {

  },
  "pipeline": [
    {
      "name": "build",
      "commands": [
        "cd /root/project/unityBuild",
        "source ./preBuild.sh",
        "./before_script.sh",
        "./build.default2.sh"
      ]
    }
  ],
  "cleanup": {
    "remove_build_source": true,
    "7z_build": true,
    "7z_hash": "sha1"
  }
}