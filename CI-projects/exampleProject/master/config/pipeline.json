{
  "docker": {
    "image": "gableroux/unity3d:2018.4.2f1-windows",
    "args": "-it --rm",
    "project-dest": "/root/project",
    "build-output-dest": "/root/project/unityBuild/Builds/StandaloneWindows"
  },
  "webhook": {
    "name": "push",
    "method": "POST",
    "project_request_name": "exampleProject",
    "authorized-actors": [
      "ashley sands"
    ],
    "master-commands": [
      "cd testCIGame/",
      "sudo git pull origin master "
    ],
    "pre-build-commands": [
      "cd testCIGame/",
      "sudo git checkout --detach {build_hash}"
    ]
  },
  "environment": {

  },
  "pipeline": [
    {
      "name": "build",
      "commands": [
        "cd /root/project/unityBuild",
        "source ./preBuild.sh",
        "./before_script.sh",
        "./build.default2.sh"
      ]
    }
  ],
  "cleanup": {
    "remove_build_source": "true",
    "7z_build": "true",
  }
}