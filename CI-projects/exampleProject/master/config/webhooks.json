{
  "in-webhooks":[
    {
      "name": "master",
      "authorized-actors": [
        "git user"
      ]
    }
  ],
  "out-webhooks": [
    {
      "hook-name": "Build Complete Discord",
      "type": "discord",
      "trigger": "build-complete",
      "url": "",
      "data": {
        "default": {
          "content": ""
        },
        "embed": {
          "title": "{project} - Build Complete",
          "description": "Build {buildId} has completed!",
          "fields": [
            {
              "name": "Build Name",
              "value": "{buildName}",
              "inline": false
            },
            {
              "name": "Status",
              "value": "{status}",
              "inline": true
            },
            {
              "name": "Triggered Actor",
              "value": "{actor}",
              "inline": true
            },
            {
              "name": "Output log",
              "value": "{output}",
              "inline": true
            },
            {
              "name": "7z",
              "value": "{7z}",
              "inline": true
            }
          ]
        }
      }
    }
  ]
}