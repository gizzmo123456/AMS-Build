{
  "in-webhooks":[
    {
      "name": "master",
      "authorized-actors": [
        "git user"
      ]
    }
  ],
  "out-webhooks": [
    {
      "hook-name": "Build Complete Discord",
      "type": "discord",
      "trigger": "build-complete",
      "url": "",
      "data": {
        "default": {
          "content": "Helloo World Im A bot"
        },
        "embeds":[
          {
            "title": "{project} - Build Complete",
            "description": "Build {buildId} has completed!",
            "fields": [
              {
                "name": "Build Name",
                "value": "{buildName}",
                "inline": false
              },
              {
                "name": "ID",
                "value": "{buildId}",
                "inline": true
              },
              {
                "name": "Status",
                "value": "{status}",
                "inline": true
              },
              {
                "name": "Triggered By",
                "value": "{actor}",
                "inline": true
              },
              {
                "name": "Output log",
                "value": "[View](https://{output})",
                "inline": true
              },
              {
                "name": "7z",
                "value": "[Download](https://{7z})",
                "inline": true
              }
            ]
          }
        ]
      }
    }
  ]
}